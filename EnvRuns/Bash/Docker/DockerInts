#!/bin/bash

#descriptive names for incoming params
amqBroker=$1
dest=$2
indest=$3

#------Scheduling INCOMING
#cmd/DockerRunInt $amqBroker insched 5100 3E6C7FF1-32DF-4699-8D85-06F59809F956 DCFA837E-DF14-45E0-A440-38B2FDA1985B ${indest}/559507 1
cmd/DockerRunInt $amqBroker insched 5100 3E6C7FF1-32DF-4699-8D85-06F59809F956 DCFA837E-DF14-45E0-A440-38B2FDA1985B http://epic24212.dhcp.epic.com:5015/hl7listener/incoming 1

#------Errors
cmd/DockerRunInt $amqBroker error 5200 C95808C1-8597-43E4-83DD-757CDF283574 8FA5C77E-DAB1-46F2-8642-14D69415AF89 ${dest}/334999 1

#------Orders
cmd/DockerRunInt $amqBroker order 5300 4C92FB81-769E-44E4-B0FE-9AEAB04A84DD FEC46370-0CB1-436B-BFF1-2898E5C4F8D6 ${dest}/334375 1

#------Scheduling
cmd/DockerRunInt $amqBroker sched 5400 74B57C05-A581-4E79-8423-FE863D72FE49 332A8690-D087-4E8F-871E-0B71A2D44E3F ${dest}/334501 1

#------Demographics ADT
